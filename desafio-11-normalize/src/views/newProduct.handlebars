
<div class="container">
  <h1>Ingrese su producto</h1>
  <form id="productForm">
    <label class="form-label">Producto</label>
    <input id="title" name="title" class="form-control" required />
    <label class="form-label">Precio</label>
    <input id="price" name="price" class="form-control" required />
    <br />
    <input id="file" type="file" name="file" class="form-control" required />
    <br />
    <input type="submit" class="btn btn-success" />
  </form>
  <hr>

<br />
<a href="http://localhost:8080/productos" class="fs-3"> Ver Productos</a>
<br />

<table class="table table-striped">
  <thead>
    <tr>
      <th scope="col" class="fs-6"># ID</th>
      <th scope="col" class="fs-6">Producto</th>
      <th scope="col" class="fs-6">Precio $</th>
      <th scope="col" class="fs-6">Imagen</th>
    </tr>
  </thead>
  <tbody id="mostarProductos">
    
  </tbody>
</table>

<br>
<div>
   <hr>
   <h2>Chatbox</h2>
  <form id="chatBox">
    <input type="text" id="textChat" style="width: 800px" />
    <button type="submit" id="boton">Enviar</button>
  </form>

  <div id="mostrarChat"></div>
  <br>
</div>
</div>

<script src="//cdn.jsdelivr.net/npm/sweetalert2@11"></script>
<script src="/socket.io/socket.io.js"></script>
<script src="js/newProduct.js"></script>